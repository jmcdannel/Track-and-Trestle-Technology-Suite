      "status": "fail"
    },
    {
      "id": "betatrack-layout",
      "type": "serial",
      "device": "betatrack-serial",
      "serial": "/dev/tty.usbserial-230",
      "baud": 115200,
      "status": "fail"
    },
    {
      "id": "audio",
      "type": "audio",
      "device": "MacMini",
      "connection": {}
    }
  ],
  "apiHost": "localhost",
  "jmri": "http://localhost:12080/json/"
}