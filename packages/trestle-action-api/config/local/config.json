{
  "layoutId": "betatrack",
  "name": "Beta Track Junction",
  "modules": [
    "turnouts",
    "routes",
    "locos",
    "effects"
  ],
  "devices": [
    {
      "type": "arduino",
      "id": "cmd",
      "desc": "Command Station EX"
    },
    {
      "type": "arduino",
      "id": "betatrack-serial",
      "desc": "Arduino Direct Serial Control"
    },
    {
      "type": "macOS",
      "id": "MacMini",
      "desc": "MacMini w/JMRI",
      "connectedDevices": [
        "cmd",
        "betatrack-serial"
      ]
    }
  ],
  "interfaces": [
    {
      "id": "cmd-serial",
